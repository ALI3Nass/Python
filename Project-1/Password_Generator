from operator import length_hint
import random

print("Welcome to password genrator")

chars = ''' qwertyuipoasdfghjklzxcvbnmQWERTYUIOPASDFGHJKLZXCVBNM1234567890!#$%@'''



length = input("Enter the Length of password you want: ")
length = int(length)

print("\n here are your passwords: \n")

for pwd in range(length):
    password = ""
    for c in range(length):
        password += random.choice(chars)
    
print(password)
print(' ')
